[alias]
  praise = blame
  s = status
  a = add
  praise = blame
  aa = ! git add . && git s
  b = branch
  r = reset
    ra = ! git r . && git s
  f = fetch
  m = merge
  cm = commit -m
  cam = commit -am
  co = checkout
    coa = ! git co . && git s
  ps = push
  pom = pull origin master
  pl = pull
  d = diff
    ds = diff --staged
  lg = log -p
  lgs = log --pretty='format:%C(yellow)%h%C(cyan)%Creset was %C(cyan)%an%C(white), %ar, message: %s%Creset'
  graph = log --graph --pretty='format:%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset %C(blue){%an}%Creset' --abbrev-commit --date=relative
  flc = commit --amend -a
  np = log master.. --oneline
  npr = log origin/master.. --oneline
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
[user]
  name = Matt Bridges
  email = mbridges.91@gmail.com
[color]
  ui = true
[push]
  default = current
[url "git@heroku.com:"]
  insteadOf = heroku:
[url "git://github.com/"]
  # Read-only
  insteadOf = gh:
[url "git@github.com:"]
  # With write access
  insteadOf = wgh:
[core]
  excludesfile = /Users/mattbridges/.gitignore_global
